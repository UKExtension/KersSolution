@{
    Layout = "~/Views/Shared/_LayoutReports.cshtml";
    if(ViewData["Title"] == null){
        ViewData["Title"] = "Statistical Cotnacts Per Person";
    }
    if(ViewData["Subtitle"] == null){
        ViewData["Subtitle"] = " Statistical Contact Reports";
    }
    var type = ViewData["Type"] as int?;
}
@model Kers.Models.ViewModels.TableViewModel
<div class="text-right">
    @if( @type == 0){
        <a asp-controller="Contacts"  asp-action="Districts" class="btn btn-default btn-xs">Back to Districts</a><br>
    }else{
        <a asp-controller="Contacts"  asp-action="Index" class="btn btn-default btn-xs">Back to State Reports</a><br>
    }
    
</div>
@Html.Partial("TablePartial")