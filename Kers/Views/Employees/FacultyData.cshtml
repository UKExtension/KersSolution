@{
    Layout = "~/Views/Shared/_LayoutReports.cshtml";
    var FiscalYear = ViewData["FiscalYear"] as Kers.Models.Entities.KERScore.FiscalYear;
    if(ViewData["Title"] == null){
        ViewData["Title"] = "Statistical Contacts Per Person";
    }
    ViewData["Title"] += " Report for FY" + FiscalYear.Name;
    if(ViewData["Subtitle"] == null){
        ViewData["Subtitle"] = "Statistical Contacts";
    }
    ViewData["Subtitle"] = FiscalYear.Start.ToString("MMM d, yyyy") + " - " + FiscalYear.End.ToString("MMM d, yyyy");
    

}
@model Kers.Models.ViewModels.TableViewModel

@await Html.PartialAsync("TablePartial")